{
  "manifest_version": 3,
  "name": "WhatsApp Web Contact Blur",
  "version": "1.1",
  "description": "Blurs the contacts list on WhatsApp Web for privacy",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["content.js"]
    }
  ]
}
